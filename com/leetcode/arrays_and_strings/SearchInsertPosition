
package com.leetcode;

/**
 * Algo:
 * 1. Iterate through sorted array
 *  a) if match return index, else if target<nums[i] return index aka if inserted the element will be here
 *  b) otherwise, element is greater than all elements and needs to be placed at end
 */
public class Program {
    public static int searchInsert(int[] nums, int target) {
        int index=0;
        for(;index<nums.length; index++) {
            if (target==nums[index] || target<nums[index])
                return index;
        }

        return index;
    }

    public static void main(String[] args) {
        //Tests
        searchInsert(new int[] {1, 2, 4}, 5); //1, 4, 2, 5
    }
}